<!-- Modal -->
<div class="modal fade" id="new_group" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title" id="myModalLabel">New Group</h4>
      </div>
      <div class="modal-body">
       
  
          <input type="text" ng-model="new_group" name="text" class="span3" id="group" placeholder="New Group" />
  
        
      </div>
      <div class="modal-footer">
          <button id="edittimetable" class="btn btn-success" data-dismiss="modal" ng-click="createGroup()"><i class="fa fa-plus-square"></i>Create</button>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->


